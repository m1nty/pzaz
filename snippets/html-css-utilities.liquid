	--cr-purple-light: #FC24EB;
	--cr-green-light: #37E700;
	--cr-blue-light: #00D0EE;
	--cr-red-light: #FE2A6B;
	
	--t-h-1: 700 48px/1.3 "Druk", Helvetica, sans-serif;
	--t-h-2: 700 24px/1.3 "Druk", Helvetica, sans-serif;
	--t-h-3: 700 20px/1.3 "Druk", Helvetica, sans-serif;
	--t-b-1: italic bold 22px/1.1 "Space Grotesk", Helvetica, sans-serif;
	--t-b-2: 16px/1.1 "Space Grotesk", Helvetica, sans-serif;
	--t-b-3: 11px/1.1 "Space Grotesk", Helvetica, sans-serif;
	--t-l-1: 18px/1.3 "Druk", Helvetica, Helvetica, sans-serif;
    --t-l-2: 700 30px/1.3 "Druk", Helvetica, sans-serif;
    --t-b-4: 700 20px/1.2 "Space Grotesk", Helvetica, sans-serif;
    --t-h-4: 700 96px/1.3 "Druk", Helvetica, sans-serif;

	--s-1: 5px;
	--s-2: 10px;
	--s-3: 15px;
	--s-4: 25px;
	--s-5: 40px;
	--s-6: 80px;
	--s-7: 120px;
	--s-8: 160px;

	--s-section: var(--s-5);
	--s-section-half: var(--s-4);
	--s-contain: 4vw;
	--s-contain-max: 1600px;
	--s-edge: var(--s-contain);

	--s-header: 60px;

	--a-panel: cubic-bezier(0.36, 0.07, 0.19, 0.97);
	--a-bounce: cubic-bezier(0.5, -0.5, 0.5, 1.5);
	--a-swift: cubic-bezier(0, 0.99, 0.6, 0.99);
}

@media (max-width: 600px) {
	:root {
		
		--t-h-1: 700 36px/1.3 "Druk", Helvetica, sans-serif;
		--t-h-2: 700 22px/1.3 "Druk", Helvetica, sans-serif;
		--t-h-3: 700 18px/1.3 "Druk", Helvetica, sans-serif;
		--t-b-1: italic bold 20px/1.1 "Space Grotesk", Helvetica, sans-serif;
		--t-b-2: 14px/1.1 "Space Grotesk", Helvetica, sans-serif;
		--t-l-1: 16px/1.3 "Druk", Helvetica, Helvetica, sans-serif;
	}
}

@media (max-width: 375px) {
	:root {
		--t-l-1: 16px/1.3 "Druk", Helvetica, Helvetica, sans-serif;
	}
}

::selection {
	text-shadow: none;
	background-color: rgba(125, 125, 125, 0.3);
}

::-moz-selection {
	text-shadow: none;
	background-color: rgba(125, 125, 125, 0.3);
}

@media screen and (max-width: 600px) {

	:root {
		--s-section: var(--s-4);
		--s-section-half: var(--s-3);
	}

}

/*	
	update edge spacing when .c reaches its max width	
	1600 var(--s-contain-max) / 1 - var(--s-contain)*2
*/

@media screen and (min-width: 1740px) {

	:root {
		--s-edge: calc((100vw - var(--s-contain-max))/2);
	}

}

@media (hover: hover) {

	* {
		outline-offset: 3px;
		outline-color: transparent;
	}

	*:focus {
		outline: 1px dotted rgba(125, 125, 125, 0.8);
	}

}

