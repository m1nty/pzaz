/*	U3 TYPOGRAPHY	*/

body {
	font: var(--t-b-2);
	line-height: 1;
	-webkit-font-smoothing: subpixel-antialiased;
}

	h1,
	h2,
	h3,
	h4,
	h5,
	h6 {
		font: var(--t-h-1);
	}

	.t-h-1 {
		font: var(--t-h-1);
	}

	.t-h-2 {
		font: var(--t-h-2);
	}

	.t-h-3 {
		font: var(--t-h-3);
	}

	p,
	ol,
	ul {
		font: var(--t-b-2);
	}

	.t-b-1 {
		text-transform: lowercase;
		font: var(--t-b-1);
	}

	.t-b-2 {
		font: var(--t-b-2);
	}

	.t-b-3 {
		font: var(--t-b-3);
	}

	.t-l-1 {
		text-transform: uppercase;
		font: var(--t-l-1);
	}

	.caps {text-transform: uppercase;}

	p a {text-decoration: underline;}


.btn {
	position: relative;
	display: flex;
	align-items: center;
	text-transform: uppercase;
  justify-content: center;
	white-space: nowrap;
	padding: var(--s-2) var(--s-3);
	width: max-content;
	min-width: 170px;
	flex: none;
	background-color: var(--cr-white);
	color: var(--cr-black);
	border-radius: 10px;
	font: var(--t-l-1);
	text-align: center;
	border: var(--cr-black) 5px solid;
	box-shadow: none;
	outline: none;
	cursor: pointer;
	will-change: transform;
	user-select: none;
	transition: .15s linear;
	-webkit-filter: drop-shadow(0 7px 0 var(--cr-black));
	filter: drop-shadow(0 7px 0 var(--cr-black));
}

	/* text node line height fix */
	.btn::before {
		content: "";
		display: inline-block;
		height: 20px;
		vertical-align: middle;
	}


	@media (hover: hover) {
		.btn:hover {
			transform: translateY(3px);
			-webkit-filter: drop-shadow(0 4px 0 var(--cr-black));
			filter: drop-shadow(0 4px 0 var(--cr-black));
			background-color: var(--cr-yellow);
		}
	}
  
        .btn:focus {
			background-color: var(--cr-yellow);
		}

	.btn:active {
		transform: translateY(7px);
		-webkit-filter: drop-shadow(0 0 0 var(--cr-black));
			filter: drop-shadow(0 0 0 var(--cr-black));
        background-color: var(--cr-yellow);
	}

	@media (max-width: 375px) {
		.btn {
			min-width: 150px;
            font-size: 14px;
		}
	}
  #Capa_1{
  	height: 18px;
  }
@media (max-width: 605px)
  #Capa_1 {
    height: 14px;
  }
}


